{
  "manifest_version": 2,
  "default_locale": "en",
  "name": "Open Last Tab",
  "short_name": "OLT",
  "description": "__MSG_description__",
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "commands": {
    "alt_switch_fast": {
      "suggested_key": {
        "default": "Alt+Q",
        "mac": "Alt+Q"
      },
      "description": "__MSG_alt_switch_fast_description__"
    },
    "alt_switch_slow_backward": {
      "suggested_key": {
        "default": "Alt+S",
        "mac": "Alt+S"
      },
      "description": "__MSG_alt_switch_slow_backward_description__"
    },
    "alt_switch_slow_forward": {
      "suggested_key": {
        "default": "Alt+Shift+S",
        "mac": "Alt+Shift+S"
      },
      "description": "__MSG_alt_switch_slow_forward_description__"
    }
  },
  "browser_action": {
    "default_title": "__MSG_title__",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}